{
  "name": "promptu",
  "version": "5.0.0",
  "description": "Micro CSS-in-JS library",
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf build lib",
    "pages": "rimraf .gh-pages && typedoc --excludeExternals --hideGenerator --darkHighlightTheme dark-plus --name Promptu --includeVersion --out ./.gh-pages ./src && touch ./.gh-pages/.nojekyll",
    "build": "npm run clean && webpack --config config/build.conf.ts && ts-node scripts/generateNormalizeCSS",
    "test": "mocha --require ts-node/register --timeout 10000 --reporter spec --full-trace --colors --recursive --bail --exit \"tests/**/*.ts\"",
    "lint": "eslint --ext .ts --ext .ts config src tests",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewscwei/promptu.git"
  },
  "keywords": [
    "css",
    "js"
  ],
  "files": [
    "build",
    "lib"
  ],
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewscwei/promptu/issues"
  },
  "homepage": "https://github.com/andrewscwei/promptu#readme",
  "types": "build/index.d.ts",
  "devDependencies": {
    "@types/css-tree": "^1.0.6",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.5",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "css-tree": "^1.1.3",
    "eslint": "^8.1.0",
    "mocha": "^9.1.3",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.7",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {}
}
